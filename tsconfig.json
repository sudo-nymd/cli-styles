{
  "compilerOptions": {
    "target": "es2016",
    "declaration": true,
    "declarationDir": "types",
    "sourceMap": true,
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "outDir": "lib",
  }, "include": [
    "./src/**/*.ts"
  ]
}